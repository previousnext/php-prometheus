{
  "name": "previousnext/php-prometheus",
  "description": "A PHP library for serializing to the prometheus text format.",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Kim Pepper",
      "email": "kim@previousnext.com.au"
    }
  ],
  "require": {
    "php": "^8.1|^8.2",
    "ext-json": "*",
    "symfony/property-access": "^4.4|^5.4|^6.2",
    "symfony/serializer": "^4.4|^5.4|^6.2"
  },
  "require-dev": {
    "drupal/coder": "^8.3.16",
    "phpunit/phpunit": "^9.5.28",
    "phpspec/prophecy": "^1.16",
    "symfony/phpunit-bridge": "^5.4"
  },
  "autoload": {
    "psr-4": {"PNX\\Prometheus\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"PNX\\Prometheus\\Tests\\": "tests/"}
  },
  "config": {
    "bin-dir": "bin/",
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
